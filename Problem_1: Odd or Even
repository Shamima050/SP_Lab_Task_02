#include<stdio.h>

int main(){
    int x;

    printf("Enter the number: ");
    scanf("%d",&x);

    if(x<0){
        printf("Invalid number");
    }
    else{
        if(x == 0){
            printf("Zero\n");
        }
        else if(x%2 == 0){
            printf("Even\n");
        }
        else if(x%2 != 0){
            printf("Odd\n");
        }
    }

    return 0;
}
