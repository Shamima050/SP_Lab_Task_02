#include<stdio.h>

int main(){
    int n,i,max;
    printf("enter the number of elements in the array: \n");
    scanf("%d", &n);

    int arr[n];

    printf("enter the elements of the array: \n");
    for(i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }

    max=arr[0];
    for(i=1; i<n;i++){
        if(arr[i]>max){
            max=arr[i];
        }

    }
    printf("the maximum element of the array is: %d",max);

    return 0;
}
